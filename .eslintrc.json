{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": true
    },
    "rules": {
        "@typescript-eslint/no-floating-promises": 0,
        "@typescript-eslint/consistent-type-definitions": 0,
        "@typescript-eslint/no-namespace": 0
    }
}
